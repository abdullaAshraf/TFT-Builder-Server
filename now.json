{
    "version": 2,
    "name": "TFT",
    "builds": [
        {
            "src": "**/*.js",
            "use": "@now/node"
        },
        {
            "src": "**/*.html",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}